      "bundleIdentifier": "org.kopfsachen.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CAE2E2"
      },
      "package": "org.kopfsachen.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "2ed357a3-8d38-431d-bfaa-a6ed64e89caf"
      }
    }
  }
