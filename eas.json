{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "releaseChannel": "development",
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "releaseChannel": "staging",
      "env": {
        "NODE_ENV": "staging"
      },
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "withoutCredentials": "true"
      },
      "distribution": "internal"
    }
  }
}
